{
    "manifest_version": 3,
    "name": "Duolingo Accent Keycode Helper",
    "version": "1.0",
    "description": "Shows keycodes for accented characters on Duolingo.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.duolingo.com/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  